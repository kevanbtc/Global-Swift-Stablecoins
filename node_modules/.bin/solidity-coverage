#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Kevan/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.5_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_ty_t3ezr7vjy62qmtulwnk5fii4je/node_modules/solidity-coverage/plugins/node_modules:/mnt/c/Users/Kevan/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.5_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_ty_t3ezr7vjy62qmtulwnk5fii4je/node_modules/solidity-coverage/node_modules:/mnt/c/Users/Kevan/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.5_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_ty_t3ezr7vjy62qmtulwnk5fii4je/node_modules:/mnt/c/Users/Kevan/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Kevan/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.5_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_ty_t3ezr7vjy62qmtulwnk5fii4je/node_modules/solidity-coverage/plugins/node_modules:/mnt/c/Users/Kevan/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.5_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_ty_t3ezr7vjy62qmtulwnk5fii4je/node_modules/solidity-coverage/node_modules:/mnt/c/Users/Kevan/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.5_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_ty_t3ezr7vjy62qmtulwnk5fii4je/node_modules:/mnt/c/Users/Kevan/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.5_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_ty_t3ezr7vjy62qmtulwnk5fii4je/node_modules/solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.5_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_ty_t3ezr7vjy62qmtulwnk5fii4je/node_modules/solidity-coverage/plugins/bin.js" "$@"
fi
