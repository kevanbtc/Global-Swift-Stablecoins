{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Broker Fills (UST Bills/Notes)",
  "type": "object",
  "required": ["trade_date","settle_date","cusip","maturity","face","price","account"],
  "properties": {
    "trade_date": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
    "settle_date": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
    "cusip": { "type": "string" },
    "maturity": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
    "face": { "type": "string", "pattern": "^[0-9,]+(\\.[0-9]+)?$" },
    "price": { "type": "string" },
    "account": { "type": "string" }
  },
  "additionalProperties": true
}
